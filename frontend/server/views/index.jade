                        li
                          a(href='#') Art
                        li
                          a(href='#') Software
                        li
                          a(href='#') Hardware
                    li
                      a(href='contacts') About Us
                nav.nav.put-right
                  ul.sf-menu
                    li
                      form.navbar-form.navbar-right(role='search')
                        .form-group
                          input.form-control(type='text', placeholder='Search')
                        button#headerSearchButton.btn.btn-default(type='submit')
                          a(href='searchproject')
                            span.glyphicon.glyphicon-search
                    // <li class="current"><a href="services">Services</a></li>
                    li
                      a(href='login') Log in
                    li
